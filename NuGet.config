<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="docsvision" value="https://packages.docsvision.com/nuget/docsvision/index.json" />
	<add key="docsvision-deps" value="https://packages.docsvision.com/nuget/docsvision-deps/index.json" protocolVersion="3" />
  </packageSources>
  <packageSourceMapping>
    <clear />
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="docsvision">
      <package pattern="DocsVision.*" />
    </packageSource>
    <packageSource key="docsvision-deps">
      <package pattern="DevExpress.*" />
    </packageSource>	
  </packageSourceMapping>
</configuration>